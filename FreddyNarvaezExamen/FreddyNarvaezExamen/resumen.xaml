<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FreddyNarvaezExamen.resumen">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="lblUser" HorizontalTextAlignment="End" ></Label>
            <Label Text="Nombre del Estudiante" HorizontalTextAlignment="Center" TextColor="Black"
         FontSize="25"></Label>
            <Label x:Name="lblnombre" HorizontalTextAlignment="Center" FontSize="20" ></Label>
            <Label Text="Pago Mensual por tres Meses" HorizontalTextAlignment="Center" TextColor="Black"
         FontSize="25"></Label>
            <Label x:Name="lblpago" HorizontalTextAlignment="Center" FontSize="20"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>